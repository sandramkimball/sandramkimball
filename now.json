{
    "version": 2,
    "name": "portfolio_react",
    "builds": [
        { 
            "src": "portfolio/package.json", 
            "use": "@now/static-build", 
            "config": {"distDir": "build"} 
        }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "portfolio_react/$1" 
        }
    ]
  }
